u3000\u898b\u3089\u308c\u305f\uff01\uff1f\n\u3000\u3044\u3084\u3001\u5098\u3067\u898b\u3048\u306a\u304b\u3063\u305f\u2026\u2026\u3088\u306d\u3002\n\n\u300c\u671d\u304b\u3089\u898b\u305b\u3064\u3051\u3066\u304f\u308b\u306d\u3001\u3075\u305f\u308a\u3068\u3082\u300d\n\n\u3000\u3046\u308f\u30fc\uff01\u3000\u898b\u3089\u308c\u3066\u305f\u30fc\uff01\n\n\u300c\u306a\u3001\u306a\u306a\u3001\u306a\u3093\u3067\uff1f\u3000\u5098\u3067\u898b\u3048\u306a\u3044\u3001\u306f\u305a\u300d\n\u300c\u305d\u3046\uff1f\u3000\u666e\u901a\u306b\u898b\u3048\u3066\u305f\u3088\u3002\u3042\u3001\u592a\u90ce\u541b\u304c\u8ab0\u304b\u3068\u3044\u3061\u3083\u3064\u3044\u3066\u308b\u306d\u3063\u3066\u300d\n\n\u3000\u8ab0\u304b\u3063\u3066\u2026\u2026\u3002\n\u3000\u51db\u306f\u898b\u3048\u3066\u306a\u304b\u3063\u305f\u307f\u305f\u3044\u3060\u3002\n\n\u300c\u3042\u3001\u51db\u3061\u3083\u3093\u3002\u3044\u305f\u306e\uff1f\u300d\n\u300c\u2026\u2026\u5f53\u305f\u308a\u524d\u3058\u3083\u3093\u3002\u4ed6\u306b\u305f\u308d\u30fc\u3068\u4e00\u7dd2\u306b\u5c45\u308b\u4eba\u306a\u3093\u3066\u3044\u306a\u3044\u3067\u3057\u3087\uff1f\u300d\n\n\u3000\u51db\u3055\u3093\u305d\u308c\u306f\u3061\u3087\u3063\u3068\u50b7\u3064\u304f\u3088\u3002\n\n\u300c\u3075\u305f\u308a\u3068\u3082\u540c\u3058\u5098\u306b\u5165\u3063\u3066\u3001\u4f55\u3057\u3066\u308b\u306e\u304b\u306a\uff1f\u300d\n\u300c\u898b\u3066\u308f\u304b\u3089\u306a\u3044\uff1f\u3000\u671d\u304b\u3089\u305f\u308d\u30fc\u3068\u3044\u3061\u3083\u3044\u3061\u3083\u3057\u3066\u308b\u306e\u3002\u90aa\u9b54\u3060\u304b\u3089\u5931\u305b\u308d\u300d\n\n\u3000\u6016\u3044\u3067\u3059\u51db\u3055\u3093\u3002\n\n\u300c\u306a\u308b\u307b\u3069\u3001\u5098\uff11\u672c\u3057\u304b\u306a\u3044\u3093\u3060\u3002\u3058\u3083\u3001\u592a\u90ce\u304f\u3093\u3001\u826f\u304b\u3063\u305f\u3089\u308f\u305f\u3057\u306e\u5098\u306b\u5165\u3089\u306a\u3044\uff1f\u300d\n\u300c\u2026\u2026\u306f\uff1f\u3000\u306a\u3093\u3067\u305d\u3046\u306a\u308b\u308f\u3051\uff1f\u300d\n\n\u300c\u51db\u3061\u3083\u3093\u306f\u305d\u306e\u5098\u4f7f\u3044\u306a\u3088\u300d\n\u300c\u5acc\u3002\u305c\u3063\u305f\u3044\u5acc\uff01\u3000\u305f\u308d\u30fc\u306f\u6e21\u3055\u306a\u3044\u304b\u3089\uff01\u300d\n\n\u3000\u304e\u3085\u3063\u3001\u3068\u51db\u304c\u50d5\u306e\u8155\u306b\u62b1\u304d\u7740\u304f\u3002\n\u3000\u3075\u306b\u3063\u3068\u3001\u3075\u306b\u3063\u3068\u67d4\u3089\u304b\u306a\u611f\u89e6\u304c\uff01\n\n\u300c\u592a\u90ce\u541b\u3001\u80a9\u306c\u308c\u3066\u308b\u3088\u3002\u307b\u3089\u3001\u3053\u3063\u3061\u306e\u5098\u306e\u65b9\u304c\u5927\u304d\u3044\u304b\u3089\u3001\u4e00\u7dd2\u306b\u767b\u6821\u3057\u3088\u3046\u3088\u300d\n\n\u3000\u3048\u307f\u308a\u3082\u307e\u305f\u3001\u50d5\u306b\u304e\u3085\u3063\u3068\u3057\u3066\u304f\u308b\u3002\n\u3000\u51db\u306b\u8ca0\u3051\u305a\u52a3\u3089\u305a\u5927\u304d\u306a\u80f8\u304c\uff01\n\n\u300c\u305f\u308d\u30fc\u306f\u30a2\u30bf\u30b7\u3068\u4e00\u7dd2\u306b\u3044\u304f\u306e\u300d\n\u300c\u592a\u90ce\u541b\u306f\u308f\u305f\u3057\u3068\u304c\u3044\u3044\u3063\u3066\uff01\u300d\n\n\u3000\u50d5\u306f\u3048\u307f\u308a\u3092\u898b\u3066\u8a00\u3046\u3002\n\n\u300c\u3042\u306e\u3055\u3001\u59d4\u54e1\u9577\u3002\u3044\u3044\u3088\u3001\u50d5\u3089\u306e\u3053\u3068\u306f\u6c17\u306b\u3057\u306a\u3044\u3067\u300d\n\u300c\u305d\u30fc\u3060\u3042\u3063\u3061\u3044\u3051\u304a\u90aa\u9b54\u866b\u3002\u611b\u3057\u5408\u3046\u3075\u305f\u308a\u306e\u90aa\u9b54\u3059\u308b\u306a\u300d\n\n\u3000\u3057\u3063\u3057\u3001\u3068\u51db\u304c\u866b\u3092\u6255\u3046\u3088\u3046\u306b\u624b\u3092\u632f\u308b\u3002\n\n\u300c\u52d8\u9055\u3044\u3057\u306a\u3044\u3067\u3002\u3084\u307e\u3057\u3044\u6c17\u6301\u3061\u306f\u306a\u3044\u306e\u300d\n\n\u300c\u300c\u3048\uff1f\u300d\u300d\n\n\u3000\u771f\u9762\u76ee\u306a\u9854\u3067\u3048\u307f\u308a\u304c\u8a00\u3046\u3002\n\n\u300c\u307b\u3089\u3001\u3075\u305f\u308a\u3068\u3082\u80a9\u6fe1\u308c\u3066\u308b\u3067\u3057\u3087\u3002\u3064\u307e\u308a\u305d\u306e\u5098\u3058\u3083\u72ed\u3044\u3063\u3066\u3053\u3068\u3002\u3053\u306e\u307e\u307e\u98a8\u90aa\u3092\u3072\u304f\u306e\u3092\u3001\u30af\u30e9\u30b9\u59d4\u54e1\u3068\u3057\u3066\u898b\u904e\u3054\u305b\u306a\u3044\u3060\u3051\u300d\n\n\u3000\u7d50\u69cb\u307e\u3068\u3082\u306a\u7406\u7531\u3060\u3063\u305f\u3002\n\n\u300c\u3068\u3044\u3046\u3053\u3068\u3067\u3001\u307b\u3089\u3001\u592a\u90ce\u541b\u3002\u3053\u3063\u3061\u304a\u3044\u3067\u300d\n\u300c\u305c\u3063\u305f\u3044\u3001\u5acc\uff01\u3000\u3084\u3063\u3071\u308a\u90aa\u306a\u8003\u3048\u304c\u3042\u308b\u3093\u3058\u3083\u3093\uff01\u300d\n\n\u3000\u304e\u3083\u3042\u304e\u3083\u3042\u3001\u3068\u53e3\u8ad6\u306b\u306a\u308b\u3075\u305f\u308a\u3002\n\u3000\u3042\u3042\u3082\u3046\u3001\u3069\u3046\u3059\u308c\u3070\u3044\u3044\u3093\u3060\u2026\u2026\u3002\n\n\u3000\u3068\u305d\u306e\u3068\u304d\u3060\u3063\u305f\u3002\n\n\u300c\u2026\u2026\u305b\u3093\u3071\u3044\u3002\u304a\u306f\u3088\u3046\u3054\u3056\u3044\u307e\u3059\u300d\n\n\u3000\u5f8c\u8f29\u306e\u3001\u6771\u5fa1(\u3068\u3046\u307f)\u7dbe\u5b50\u304c\u3001\u5098\u3092\u3055\u3057\u3066\u3001\u50d5\u3089\u306e\u6a2a\u306b\u5c45\u305f\u3002\n\n\u300c\u2026\u2026\u305b\u3093\u3071\u3044\u3002\u56f3\u66f8\u59d4\u54e1\u306e\u96c6\u4f1a\u306b\u9045\u308c\u3061\u3083\u3044\u307e\u3059\u3088\uff1f\u300d\n\u300c\u3048\uff1f\u3000\u3057\u3085\u3001\u96c6\u4f1a\uff1f\u300d\n\n\u3000\u305d\u3093\u306a\u3082\u306e\u306a\u304b\u3063\u305f\u3088\u3046\u306a\u6c17\u304c\u2026\u2026\u3002\n\n\u300c\u305d\u3046\u306a\u3093\u3060\u3001\u3054\u3081\u3093\u306d\u592a\u90ce\u541b\u3002\u5f15\u304d\u7559\u3081\u3061\u3083\u3063\u3066\u300d\n\u300c\u305f\u308d\u30fc\u3001\u5148\u884c\u3063\u3066\u300d\n\n\u3000\u3042\u3001\u3042\u308c\uff1f\u3000\u3053\u308c\u3082\u3057\u304b\u3057\u3066\u3001\u7dbe\u5b50\u304c\u50d5\u3092\u52a9\u3051\u3066\u304f\u308c\u3066\u308b\uff1f\n\n\u300c\u2026\u2026\u305b\u3093\u3071\u3044\u884c\u304d\u307e\u3057\u3087\u3046\u300d\n\